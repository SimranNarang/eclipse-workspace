package com.stackroute.datamunger.query.parser;

/*
 * This class is used for storing name of field, condition and value for 
 * each conditions
 * */
public class Restriction {
	private String propertyName;
	private String propertyValue;
	private String condition;
	public Restriction(String properyName, String properyValue, String condition) {
		super();
		this.propertyName = properyName;
		this.propertyValue = properyValue;
		this.condition = condition;
	}
	public String getPropertyName() {
		return propertyName;
	}
	public void setProperyName(String properyName) {
		this.propertyName = properyName;
	}
	public String getProperyValue() {
		return propertyValue;
	}
	public void setProperyValue(String properyValue) {
		this.propertyValue = properyValue;
	}
	public String getCondition() {
		return condition;
	}
	public void setCondition(String condition) {
		this.condition = condition;
	}
	

}
